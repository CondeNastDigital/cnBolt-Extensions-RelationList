{
    "name": "bolt/thumbs",
    "description": "Small package based on GD to handle thumbnail resizes",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Riley",
            "email": "riley.ross@gmail.com"
        },
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "ext-gd": "*",
        "silex/silex": "^1.3",
        "bolt/filesystem": "^2.3",
        "contao/imagine-svg": "~0.1.2",
        "doctrine/cache": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.7",
        "symfony/browser-kit": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Bolt\\Thumbs\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Thumbs\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-3.x": "3.4-dev"
        }
    }
}
